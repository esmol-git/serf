section.holder
    .container
        .title 
            span travel 
                h3 travel
        .holder__slider
            .holder__slider-item.holder-slider
                .holder-slider__head(style={"background-image":"url(../static/images/content/travel/slider/travel-bg_1.png)"})
                    .holder-slider__subtitle
                        .subtitle
                            .headline-left Shore
                            .subtitle__head Airlie Beach <span>|</span> Australia
                .holder-slider__descr(style={"background-image":"url(../static/images/content/travel/airline.png)"})
                    .descr
                        .headline-right Airline
                        .descr__head Virgin Australia
                            img(src="static/images/content/travel/airline-logo.png", alt="airline-logo")
                .holder-slider__info
                    .holder-slider__info-item
                        .holder-slider__info-subtitle Destination
                        .holder-slider__info-title Queensland <br> Australia
                    .holder-slider__info-item
                        .holder-slider__info-subtitle Distance
                        .holder-slider__info-title 7,065 Miles
                    .holder-slider__info-item
                        .holder-slider__info-subtitle Travel Time
                        .holder-slider__info-title 23 Hours <br> 5 Minutes
                    .holder-slider__info-item
                        .holder-slider__info-subtitle Pricing
                        .holder-slider__info-title $1,976 USD
                            span.holder-pricing__span Round Trip
                .holder-slider__btn   
                    a(href="#").btn book
                        span flight    
